<div class="container">
  <h2>Menu Options <%= link_to "(+)", new_restaurant_menu_path(@restaurant) %></h2>

  <ul>
    <%@menus.each do |menu| %>
    <li>
      <%=link_to menu.name, edit_restaurant_menu_path(@restaurant, menu) %>
      <td><%= link_to "Delete", restaurant_menu_path(@restaurant, menu),class: "btn btn-default", method: :delete%></td>
    </li>
    <%end%>
  </ul>
</div>
