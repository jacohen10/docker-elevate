<%= form_for @restaurant do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :contact %>
<%= f.text_field :contact %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :phone %>
<%= f.text_field :phone %>
<%= f.label :menu %>
<%= f.text_field :menu %>
<%= f.submit%>
<%end%>
