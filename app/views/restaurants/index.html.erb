<div class="jumbotron vertical-center">
<% if current_user.role == "admin"%>
  <h2>Restaurants <%= link_to "(+)", new_restaurant_path %></h2>

  <ul>
    <%@restaurants.each do |restaurant| %>
    <li>
      <%=link_to restaurant.name, restaurant_path(restaurant) %>
    </li>
    <%end%>
  </ul>
</div>
<%end%>
