<%= form_for @customer do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :year %>
<%= f.select :year, options_for_select([['2nd Year', 2], ['3rd Year', 3], ['4th Year', 4]]) %>
<%= f.label :plan %>
<%= f.select :plan, options_for_select([['10', 10], ['45', 45], ['90', 90]]) %>
<%= f.submit%>
<%end%>
