<%= form_for @customer do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :phone %>
<%= f.text_field :phone %>
<%= f.label :year %>
<%= f.select :year, options_for_select([['1st Year', 1], ['2nd Year', 2], ['3rd Year', 3], ['4th Year', 4]]) %>
<%= f.label :plan %>
<%= f.select :plan, options_for_select([['15', 15], ['45', 45], ['75', 75], ['105', 105]]) %>
<%= f.submit%>
<%end%>
